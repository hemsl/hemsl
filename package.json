{
  "name": "hemsl",
  "version": "1.0.4",
  "description": "Node.js command line argv parser",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "mocha": "mocha test/*.js",
    "cover": "istanbul cover _mocha test/*.js",
    "test": "xo \"src/*.js\" \"example/**/*.js\" && npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hemsl/hemsl"
  },
  "keywords": [
    "node",
    "args",
    "parser"
  ],
  "author": "zdying <zdying@live.com>",
  "contributors": [
    "i5ting"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zdying/node-argv-parser/issues"
  },
  "homepage": "https://github.com/zdying/node-argv-parser#readme",
  "dependencies": {
    "colors": "^1.1.2"
  },
  "devDependencies": {
    "coveralls": "^2.13.0",
    "ghooks": "^2.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "xo": "^0.18.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "xo \"src/*.js\" \"example/**/*.js\""
    }
  },
  "xo": {
    "space": true,
    "globals": [
      "it", "describe"
    ],
    "no-restricted-modules": []
  }
}
